    "version": "0.2.0",
    "configurations": [
        {
            "name": "06 personen verwaltung(gdb)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/P06_Personen_Verwaltung_Linked_List/work/personen-verwaltung/bin/personen-verwaltung",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }

    ]
}